{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cppbuild",
			"label": "C/C++: g++.exe build active file",
			"command": "D:\\Conda\\CPP compiler\\bin\\g++.exe",
			"args": [
				"-g",
				"cens.cpp",
				"converter.cpp",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": {
				"kind": "build",
				"isDefault": true
			},
			"detail": "compiler: \"D:\\Conda\\CPP compiler\\bin\\g++.exe\""
		},
		{
			"type": "cppbuild",
			"label": "C/C++: gcc.exe build active file",
			"command": "D:\\Conda\\CPP compiler\\bin\\gcc.exe",
			"args": [
				"-g",
				"cens.cpp",
				"converter.cpp",
				"-o",
				"${fileDirname}\\${fileBasenameNoExtension}.exe"
			],
			"options": {
				"cwd": "${fileDirname}"
			},
			"problemMatcher": [
				"$gcc"
			],
			"group": "build",
			"detail": "compiler: \"D:\\Conda\\CPP compiler\\bin\\gcc.exe\""
		}
	]
